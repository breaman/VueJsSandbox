{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "lib": [ "es2015", "dom", "es2015.promise" ],
    "types": [ "requirejs" ]
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
